{
  "name": "photoblock",
  "version": "0.1.0",
  "description": "PhotoBlock (ALPHA Release) provides user-friendly visual account and login capability to blockchain dapps and websites",
  "browser": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\" \"npm run app\" \"npm run auth\"",
    "lib": "babel src/packages/photoblock -d lib --copy-files",
    "lib:watch": "babel src/packages/photoblock -w -d lib --copy-files",
    "app": "webpack-dev-server --config webpack.dev.app",
    "auth": "webpack-dev-server --config webpack.dev.auth",
    "prod": "webpack --config webpack.prod",
    "prod:map": "webpack --devtool source-map --config webpack.prod",
    "build:sprite": "node build/sprite.js"
  },
  "keywords": [
    "photoblock",
    "trycrypto",
    "blockchain",
    "bitcoin",
    "sso",
    "authentication",
    "login",
    "photo",
    "emoji",
    "2fa",
    "visual"
  ],
  "license": "MIT",
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/runtime-corejs2": "7.3.1",
    "babel-loader": "^8.0.0-beta.0",
    "colors": "1.3.2",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "^1.0.0",
    "exports-loader": "0.7.0",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "6.1.0",
    "imagemin-pngquant": "6.0.1",
    "imports-loader": "0.8.0",
    "jsdom": "13.0.0",
    "spritesmith": "^3.3.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack": "^4.6.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "author": {
    "name": "Nik Kalyani",
    "email": "nik@kalyani.com",
    "url": "https://www.kalyani.com"
  },
  "homepage": "https://www.trycrypto.com/photoblock",
  "repository": "github:trycrypto/photoblock",
  "dependencies": {
    "bitcoin-ts": "1.4.0",
    "blakejs": "1.1.0",
    "ethers": "4.0.26",
    "superstatic": "6.0.4",
    "tweetnacl": "1.0.1"
  }
}
